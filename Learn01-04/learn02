#include<stdio.h>
#include<ncurses.h>

int main()
{
        int num1;
        int num2;
        printf("enter a number for num1:");
        scanf("%d",&num1);
        printf("enter a number for num2:");
        scanf("%d",&num2);
        if(num1<num2)           //test-condition
        {
                printf("num1=%d\n",num1);
                printf("is smaller than\n");
                printf("num2=%d\n",num2);
        }
        else{
                printf("num2=%d\n",num2);
                printf("is smaller than num1\n");
                printf("num1=%d\n",num1);
        }
        return 0;

}
